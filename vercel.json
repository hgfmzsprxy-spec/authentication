{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)\\.html",
      "status": 301,
      "headers": {
        "Location": "/$1"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  }
}

